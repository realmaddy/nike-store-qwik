{
  "name": "nike-store-qwik",
  "version": "1.0.0",
  "description": "Modern Nike Store built with Qwik and TailwindCSS",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "echo Skipping lint && qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "qwik check-client src dist && vite build -c adapters/vercel-edge/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "clean": "rimraf dist .qwik .vite",
    "deploy": "vercel deploy",
    "dev": "vite --mode ssr",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\" || true",
    "preview": "qwik build preview && vite preview --open",
    "serve": "qwik build && serverless offline",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "dependencies": {
    "@builder.io/qwik": "^1.17.2",
    "@builder.io/qwik-city": "^1.17.2",
    "lucide-qwik": "^1.0.0",
    "lucide-react": "^0.553.0"
  },
  "devDependencies": {
    "@eslint/js": "latest",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "20.19.0",
    "autoprefixer": "^10.4.21",
    "eslint": "9.32.0",
    "eslint-plugin-qwik": "^1.17.2",
    "globals": "16.4.0",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "serverless": "^3.38.0",
    "serverless-http": "^3.2.0",
    "serverless-offline": "^13.3.2",
    "source-map-support": "^0.5.21",
    "tailwindcss": "^4.1.17",
    "typescript": "5.4.5",
    "typescript-eslint": "8.38.0",
    "typescript-plugin-css-modules": "latest",
    "undici": "*",
    "vercel": "^29.1.1",
    "vite": "^7.2.2",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
